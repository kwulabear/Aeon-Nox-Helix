    <onload condition="Skin.HasSetting(musicvisinfomode)">Skin.SetBool(infovistoggled)</onload>
    <onload condition="Skin.HasSetting(musicvisinfomode)">Skin.ToggleSetting(musicvisinfomode)</onload> 
    <onload condition="!Skin.HasSetting(DisablePanelWidgetVis)">Skin.SetBool(infovistoggled2)</onload>
